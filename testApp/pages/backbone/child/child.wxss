.prompt-wicket{
  width: 378rpx;
  height: 175rpx;
  background: #ffc649;
  color: #ffffff;
  border-radius: 20rpx;
  position: fixed;
  top: 150rpx;
  left: 180rpx;
  font-size: 30rpx;
  line-height: 175rpx;
  text-align: center;
}
.active{
  display: block;
}
.cancel {
  display: none;
}
button::after{
border: none;
}

