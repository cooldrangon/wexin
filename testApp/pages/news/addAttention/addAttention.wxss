}
.childer .who{
  margin-left: 75rpx;
  font-size:30rpx;
}
.userintro{
  color:#d2d2d2;
  position: absolute;
  bottom:10%;
  left:15%;
}
.focus{
display: inline-block;
float: right;
border-radius: 10rpx;
font-size: 24rpx !important;
padding:6rpx 20rpx;
margin-top: 21rpx;
border: 1px solid orange;
color: orange;
}
.focused{
  display: inline-block;
  float: right;
  border-radius: 10rpx;
  font-size: 24rpx !important;
