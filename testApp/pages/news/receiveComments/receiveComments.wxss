.likeCollect{
  background-color: #eeeeee;
  padding-top: 20rpx;
  height:100%;
}
.likeCollect .head{
  width:90rpx;
  height:90rpx;
  border-radius: 50%;
  float:left;
}
.likeCollect .pic{
  width:120rpx;
  height:120rpx;
  float:right;
}
.likeContent{
  background-color: #fff;
  padding-top: 45rpx;
}
.likeContent .cc{
  width:60%;
  margin-left:30rpx;
}
.cc .ctop{
  font-size: 30rpx;
}
.cc .cbottom{
  font-size: 30rpx;
  color:#d1d1d1;
  margin-bottom: 80rpx;
}
.likeContent .one{
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #eeeeee;
  padding:56rpx 30rpx;
  box-sizing: border-box;
}
.likeContent>view:last-child{
  border:0;
}

